#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/yanbinru/Desktop/代码/gitee/r-ui/node_modules/.pnpm/registry.npmmirror.com+vite@4.2.1_less@4.1.3/node_modules/vite/bin/node_modules:/Users/yanbinru/Desktop/代码/gitee/r-ui/node_modules/.pnpm/registry.npmmirror.com+vite@4.2.1_less@4.1.3/node_modules/vite/node_modules:/Users/yanbinru/Desktop/代码/gitee/r-ui/node_modules/.pnpm/registry.npmmirror.com+vite@4.2.1_less@4.1.3/node_modules:/Users/yanbinru/Desktop/代码/gitee/r-ui/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/yanbinru/Desktop/代码/gitee/r-ui/node_modules/.pnpm/registry.npmmirror.com+vite@4.2.1_less@4.1.3/node_modules/vite/bin/node_modules:/Users/yanbinru/Desktop/代码/gitee/r-ui/node_modules/.pnpm/registry.npmmirror.com+vite@4.2.1_less@4.1.3/node_modules/vite/node_modules:/Users/yanbinru/Desktop/代码/gitee/r-ui/node_modules/.pnpm/registry.npmmirror.com+vite@4.2.1_less@4.1.3/node_modules:/Users/yanbinru/Desktop/代码/gitee/r-ui/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../registry.npmmirror.com+vite@4.2.1_less@4.1.3/node_modules/vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../../../../../../registry.npmmirror.com+vite@4.2.1_less@4.1.3/node_modules/vite/bin/vite.js" "$@"
fi
