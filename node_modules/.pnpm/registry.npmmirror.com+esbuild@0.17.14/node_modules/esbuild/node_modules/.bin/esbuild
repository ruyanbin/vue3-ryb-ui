#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/yanbinru/Desktop/代码/gitee/r-ui/node_modules/.pnpm/registry.npmmirror.com+esbuild@0.17.14/node_modules/esbuild/bin/node_modules:/Users/yanbinru/Desktop/代码/gitee/r-ui/node_modules/.pnpm/registry.npmmirror.com+esbuild@0.17.14/node_modules/esbuild/node_modules:/Users/yanbinru/Desktop/代码/gitee/r-ui/node_modules/.pnpm/registry.npmmirror.com+esbuild@0.17.14/node_modules:/Users/yanbinru/Desktop/代码/gitee/r-ui/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/yanbinru/Desktop/代码/gitee/r-ui/node_modules/.pnpm/registry.npmmirror.com+esbuild@0.17.14/node_modules/esbuild/bin/node_modules:/Users/yanbinru/Desktop/代码/gitee/r-ui/node_modules/.pnpm/registry.npmmirror.com+esbuild@0.17.14/node_modules/esbuild/node_modules:/Users/yanbinru/Desktop/代码/gitee/r-ui/node_modules/.pnpm/registry.npmmirror.com+esbuild@0.17.14/node_modules:/Users/yanbinru/Desktop/代码/gitee/r-ui/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
"$basedir/../../bin/esbuild"   "$@"
exit $?
